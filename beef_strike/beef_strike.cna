sub result_tracker {
	for ($i = 0; $i < size(@beef_cmde); $i++)   {
		if (@beef_cmde[$i]["id"] eq $2 ) {
			$cmdename = @beef_cmde[$i]["name"] ;
			break;} 
	}
	%rslt = %(name => "$cmdename", ip => "$3", sessionID => "$1", commandID => "$2", resultID => "$id_result");
	@beef_result[$id_result] = %rslt;
}	

